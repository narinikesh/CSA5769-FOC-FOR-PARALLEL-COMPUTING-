#include <stdio.h>
int main() 
{
    int a, b;
    printf("Enter the first number (a): ");
    scanf("%d", &a);
    printf("Enter the second number (b): ");
    scanf("%d", &b);
    if (a > b) 
	{
        printf("The largest number is: %d\n", a);
    } 
	else if (b > a) 
	{
        printf("The largest number is: %d\n", b);
    } 
	else 
	{
        printf("Both numbers are equal.\n");
    }
    return 0;
}
