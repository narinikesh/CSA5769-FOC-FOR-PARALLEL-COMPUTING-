#include <stdio.h>
int main() 
{
    int n;
    printf("Enter the number of integers: ");
    scanf("%d", &n);
    int numbers[n];
    int i;
    for (i = 0; i < n; i++)
	 {
        printf("Enter integer %d: ", i + 1);
        scanf("%d", &numbers[i]);
    }
    int max = numbers[0];
    for (i = 1; i < n; i++)
	 {
        if (numbers[i] > max)
		 {
            max = numbers[i];
        }
    }
    printf("The largest number is: %d\n", max);
    return 0;
}
